<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="co.edu.uniquindio.poo.pfbanco.viewController.ReportesAvanzadosViewController">

   <children>
      <Label fx:id="lblTituloReportesAvanzados"
             layoutX="118.0" layoutY="20.0"
             prefHeight="58.0" prefWidth="362.0"
             text="Reportes Avanzados" textFill="#5b9954">
         <font>
            <Font size="30.0" />
         </font>
      </Label>

      <TableView fx:id="tablaReportes" layoutX="20.0" layoutY="100.0" prefHeight="250.0" prefWidth="560.0">
         <columns>
            <TableColumn fx:id="colId" text="ID" prefWidth="80.0"/>
            <TableColumn fx:id="colFecha" text="Fecha" prefWidth="120.0"/>
            <TableColumn fx:id="colMonto" text="Monto" prefWidth="100.0"/>
            <TableColumn fx:id="colEstado" text="Estado" prefWidth="100.0"/>
            <TableColumn fx:id="colOrigen" text="Origen" prefWidth="80.0"/>
            <TableColumn fx:id="colDestino" text="Destino" prefWidth="80.0"/>
         </columns>
      </TableView>
   </children>
</AnchorPane>
