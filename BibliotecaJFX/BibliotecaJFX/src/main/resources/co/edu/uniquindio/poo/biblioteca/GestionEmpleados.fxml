<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.biblioteca.viewController.GestionEmpleadosViewController">
   <children>
      <Button fx:id="BtbVolver" layoutX="144.0" layoutY="84.0" mnemonicParsing="false" onAction="#OnVolver" text="Volver" />
      <Label layoutX="42.0" layoutY="31.0" text="Gestion Empleados" textFill="#238620">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ImageView fitHeight="88.0" fitWidth="84.0" layoutX="44.0" layoutY="66.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../Imagenes/Uniqlog.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="TvEmpleado" layoutX="30.0" layoutY="170.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="TbcNombre" prefWidth="75.0" text="Nombre" />
          <TableColumn fx:id="TbcCedula" prefWidth="75.0" text="Cedula" />
        </columns>
      </TableView>
      <TextField fx:id="TxfNombre" layoutX="388.0" layoutY="54.0" />
      <TextField fx:id="TxfCedula" layoutX="389.0" layoutY="97.0" />
      <Label layoutX="314.0" layoutY="58.0" text="Nombre:" />
      <Label layoutX="314.0" layoutY="101.0" text="Cedula:" />
      <Button fx:id="BtbAgregar" layoutX="430.0" layoutY="158.0" mnemonicParsing="false" onAction="#OnAgregar" text="Agregar" />
      <Button fx:id="BtbActualizar" layoutX="434.0" layoutY="212.0" mnemonicParsing="false" onAction="#OnActualizar" text="Actualizar" />
      <Button fx:id="BtbEliminar" layoutX="442.0" layoutY="258.0" mnemonicParsing="false" onAction="#OnEliminar" text="Eliminar" />
   </children>
</AnchorPane>
