<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.biblioteca.viewController.RegistroUsuarioViewContoller">
   <children>
      <TextField fx:id="TxFNombre" layoutX="423.0" layoutY="58.0" prefHeight="26.0" prefWidth="163.0" />
      <TextField fx:id="TxFCedula" layoutX="422.0" layoutY="118.0" prefHeight="26.0" prefWidth="163.0" />
      <Label layoutX="48.0" layoutY="50.0" text="Registro Usuarios" textFill="#197b2c">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ImageView fitHeight="96.0" fitWidth="96.0" layoutX="34.0" layoutY="85.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../Imagenes/Uniqlog.jpg" />
         </image>
      </ImageView>
      <Label layoutX="349.0" layoutY="68.0" text="Nombre:">
         <font>
            <Font size="14.0" />
